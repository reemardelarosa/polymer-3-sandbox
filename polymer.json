{
  "entrypoint": "index.html",
  "shell": "polymer-sandbox.js",
  "fragments": [
    "embedded-plunker.js"
  ],
  "sources": [
   "data/**/*",
   "manifest/**",
   "package.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/**"
  ],
  "builds": [
    { "preset": "es5-bundled" },
    { "preset": "es6-unbundled", "addServiceWorker": true, "addPushManifest": true }
  ],
  "npm": true,
  "moduleResolution": "node"
}
